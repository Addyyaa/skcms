{
  "jobs": {
    "skcms": {
      "tasks": [
        "skcms-Linux",
        "skcms-Mac",
        "skcms-Win"
      ]
    },
    "skcms-Linux": {
      "tasks": [
        "skcms-Linux"
      ]
    },
    "skcms-Mac": {
      "tasks": [
        "skcms-Mac"
      ]
    },
    "skcms-Win": {
      "tasks": [
        "skcms-Win"
      ]
    }
  },
  "tasks": {
    "skcms-Linux": {
      "cipd_packages": [
        {
          "name": "infra/ninja/linux-amd64",
          "path": "ninja",
          "version": "version:1.8.2"
        },
        {
          "name": "skia/bots/android_ndk_linux",
          "path": "ndk",
          "version": "version:11"
        },
        {
          "name": "skia/bots/clang_linux",
          "path": "clang_linux",
          "version": "version:11"
        }
      ],
      "command": [
        "python",
        "skcms/infra/bots/bot.py",
        "ninja",
        "ndk",
        "clang_linux"
      ],
      "dimensions": [
        "os:Linux",
        "cpu:x86-64-Haswell_GCE",
        "gpu:none",
        "pool:Skia"
      ],
      "isolate": "bot.isolate",
      "max_attempts": 1
    },
    "skcms-Mac": {
      "cipd_packages": [
        {
          "name": "infra/ninja/mac-amd64",
          "path": "ninja",
          "version": "version:1.8.2"
        },
        {
          "name": "skia/bots/android_ndk_darwin",
          "path": "ndk",
          "version": "version:5"
        }
      ],
      "command": [
        "python",
        "skcms/infra/bots/bot.py",
        "ninja",
        "ndk"
      ],
      "dimensions": [
        "os:Mac",
        "xcode_version:9.2",
        "gpu:none",
        "pool:Skia"
      ],
      "isolate": "bot.isolate",
      "max_attempts": 1
    },
    "skcms-Win": {
      "cipd_packages": [
        {
          "name": "skia/bots/win_ninja",
          "path": "ninja",
          "version": "version:2"
        },
        {
          "name": "skia/bots/win_toolchain",
          "path": "t",
          "version": "version:8"
        },
        {
          "name": "skia/bots/clang_win",
          "path": "clang_win",
          "version": "version:6"
        }
      ],
      "command": [
        "python",
        "skcms/infra/bots/bot.py",
        "ninja",
        "t",
        "clang_win"
      ],
      "dimensions": [
        "os:Windows-2016Server",
        "gpu:none",
        "pool:Skia"
      ],
      "isolate": "bot.isolate",
      "max_attempts": 1
    }
  }
}
